#!/usr/bin/env bash

function start() {
    return 0
}

function stop() {
  return 0
}

function status(){
  return 0
}

function restart(){
  stop
  start
  return 0
}

function update() {
  return 0
}

function tidy() {
  return 0
}

function build() {
  return 0
}

function pre-build(){
 return 0
}

function post-deploy(){
  return 0
}

if ! [ $# -gt 0 ]; then
  echo "Usage: $0 [start|restart|graceful|graceful-stop|stop|status|update]"
  exit 1
fi

case "$1" in
  start) start ;;
  restart|graceful) restart ;;
  graceful-stop|stop) stop ;;
  status) status ;;
  build) build ;;
  pre-build) pre-build ;;
  post-deploy) post-deploy ;;
  tidy) tidy ;;
  update) update ;;
  *)
esac
